<p id="notice"><%= notice %></p>

<h1>Listing Catalogs Locations</h1>

<table>
  <thead>
    <tr>
      <th>Department</th>
      <th>Responsible</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogs_locations.each do |catalogs_location| %>
      <tr>
        <td><%= catalogs_location.department %></td>
        <td><%= catalogs_location.responsible %></td>
        <td><%= catalogs_location.email %></td>
        <td><%= link_to 'Show', catalogs_location %></td>
        <td><%= link_to 'Edit', edit_catalogs_location_path(catalogs_location) %></td>
        <td><%= link_to 'Destroy', catalogs_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location', new_catalogs_location_path %>
